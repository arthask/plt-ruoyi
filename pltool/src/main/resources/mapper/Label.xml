<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.pltool.mapper.LabelMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.example.pltool.domain.entity.Label">
        <id column="id" property="id" />
        <result column="uuid" property="uuid" />
        <result column="parent_uuid" property="parentUuid" />
        <result column="name" property="name" />
        <result column="create_user_id" property="createUserId" />
        <result column="create_time" property="createTime" />
        <result column="update_time" property="updateTime" />
        <result column="target" property="target"/>
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id
        , uuid, parent_uuid, name, create_user_id, create_time, update_time, target
    </sql>

</mapper>
